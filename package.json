  "scripts": {
    "start": "expo start",
    "dev:android": "expo run:android",
    "android": "expo prebuild --platform android --clean && ts-node scripts/generateWidget.ts && npx react-native-asset && expo run:android ",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "npx eas build --profile development --platform android",
    "build:local": "npx eas build --profile development --platform android --local",
    "android:clean": "cd android && ./gradlew clean && ./gradlew cleanBuildCache",
    "android:build": "cd android && ./gradlew assembleDebug",
    "android:build:release": "cd android && ./gradlew assembleRelease",
    "android:deps": "cd android && ./gradlew app:dependencies --configuration implementation",
    "android:dapp": "npx eas build -p android --profile dapp-store --local",
    "test": "echo \"No tests specified\" && exit 0",
    "eas-build-pre-install": "corepack enable && yarn set version 4.9.2",
    "prebuild:android": "node scripts/generateWidget.js",
    "build:android": "eas build --platform android"
  },
